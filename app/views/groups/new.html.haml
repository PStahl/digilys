%h1= Group.model_name.human(count: 2)

= render partial: "navigation", locals: { active: :new }

- if @copy_from
  .alert.alert-info
    = t :".copy_from_info_html",
      name: @copy_from.name,
      link: link_to(@copy_from.name, @copy_from)

= render partial: "form", object: @group, as: :group, locals: { copy_from: @copy_from }
